            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
        </goals>
        <properties>
            <exec.args>-ea -Djna.library.path=../lib -classpath %classpath ru.nsc.interval.thermocompensation.intopt.Application2 160202-GOLD-40+70 -s2 -g -manuf</exec.args>
